<div class="row-fluid">
  <div class="span3">
    <%= link_to "#{t 'back'}", other_sales_invoice_headers_path %>
  </div>
  <div class="span6" align="center">
    <h4><%= "#{t 'product'} #{t 'error_import.title'}" %></h4>
  </div>
  <div class="span3 text-right">
    <%=# link_to "kirim email", import_email_products_path(:title => @title)
     %>
  </div>
</div>

<table class="table table-condensed table-striped">
  <tr>
    <th><%= "#{t 'number'}" %></th>
    <th><%= "#{t 'barcode_id'}" %></th>
    <th><%= "#{t 'error_import.item_desc'}" %></th>
    <th><%= "#{t 'unit'}" %></th>
    <th><%= "#{t 'description'}" %></th>
  </tr>
  <% @error_rows.each do |row| %>
  <tr>
    <td><%= row[0] %></td>
    <td><%= row[1] %></td>
    <td><%= "#{row[2]} #{row[3]} #{row[4]} #{row[5]}" %></td>
    <td><%= row[7] %></td>
    <td><%= row[8] %></td>
  </tr>
  <% end %>
</table>