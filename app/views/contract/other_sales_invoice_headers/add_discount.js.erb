$("#form_discount_modal").dialog({
  autoOpen: true,
  height: "auto",
  width: "auto",
  modal: true,
  title: "<%= t 'discount' %>",
  open: function() {
    $(this).html("<%= escape_javascript(render('form_discount')) %>")
  },
  buttons: {
  	"<%= t 'cancel' %>": function() { $(this).dialog("close") }
  },
});