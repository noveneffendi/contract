$("#form_discount_modal").dialog({
  autoOpen: true,
  height: "auto",
  width: "auto",
  modal: true,
  title: "<%= t 'discount' %>",
  open: function() {
    $("#form_discount_modal").html("<%= escape_javascript(render('formdiscount')) %>")
  },
  buttons: {
  	"Cancel": function() { $(this).dialog("close") }
  },
});