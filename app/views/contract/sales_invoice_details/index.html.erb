<h1>Listing sales_invoice_details</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Unit of measure</th>
    <th>Quantity</th>
    <th>Category</th>
    <th>Sales invoice header</th>
    <th>Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sales_invoice_details.each do |sales_invoice_detail| %>
  <tr>
    <td><%= sales_invoice_detail.description %></td>
    <td><%= sales_invoice_detail.unit_of_measure %></td>
    <td><%= sales_invoice_detail.quantity %></td>
    <td><%= sales_invoice_detail.category %></td>
    <td><%= sales_invoice_detail.sales_invoice_header %></td>
    <td><%= sales_invoice_detail.number %></td>
    <td><%= link_to 'Show', sales_invoice_detail %></td>
    <td><%= link_to 'Edit', edit_sales_invoice_detail_path(sales_invoice_detail) %></td>
    <td><%= link_to 'Destroy', sales_invoice_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

