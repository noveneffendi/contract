$("#sales_order_detail_modal").dialog({
  autoOpen: true,
  height: "auto",
  width: "auto",
  modal: true,
  title: '#{t "approval"} #{t "notes"}',
  open: function() {
    $("#sales_order_detail_modal").html("<%= escape_javascript(render('approval_notes')) %>")
  },
  buttons: {
  	"<%= t 'cancel' %>": function() { $(this).dialog("close") }
  },
});