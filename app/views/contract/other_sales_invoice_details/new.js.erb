$("#other_sales_invoice_detail_modal").dialog({
  autoOpen: true,
  height: "450",
  width: "485",
  modal: true,
  title: "<%= t 'other_sales_invoice' %>-<%= t 'detail' %>-<%= t 'new' %>",
  open: function() {
    $(this).html("<%= escape_javascript(render('form')) %>")
  },
  buttons: {
  	"<%= t 'cancel' %>": function() { $(this).dialog("close") }
  },
});