<h1>Listing sales_order_details</h1>

<table>
  <tr>
    <th>Description</th>
    <th>Unit of measure</th>
    <th>Quantity</th>
    <th>Category</th>
    <th>Sales order header</th>
    <th>Number</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @sales_order_details.each do |sales_order_detail| %>
  <tr>
    <td><%= sales_order_detail.description %></td>
    <td><%= sales_order_detail.unit_of_measure %></td>
    <td><%= sales_order_detail.quantity %></td>
    <td><%= sales_order_detail.category %></td>
    <td><%= sales_order_detail.sales_order_header %></td>
    <td><%= sales_order_detail.number %></td>
    <td><%= link_to 'Show', sales_order_detail %></td>
    <td><%= link_to 'Edit', edit_sales_order_detail_path(sales_order_detail) %></td>
    <td><%= link_to 'Destroy', sales_order_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

